function(gp_resolved_file_type_override resolved_file type_var)
  if(resolved_file MATCHES "^(/Library/Frameworks/)")
        message("resolving ${resolved_file} as system")
        set(${type_var} "system" PARENT_SCOPE)
  endif()
endfunction()

